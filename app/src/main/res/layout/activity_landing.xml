<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Landing_page">

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="418dp"
        android:layout_height="625dp"
        android:layout_marginTop="-57dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/landing_page_2" />

    <TextView
        android:id="@+id/editTextTextPersonName2"
        android:layout_width="243dp"
        android:layout_height="68dp"
        android:layout_marginTop="8dp"
        android:background="@null"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="CoachMe"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="50sp"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/editTextTextPersonName"
        android:layout_width="216dp"
        android:layout_height="64dp"
        android:layout_marginTop="164dp"
        android:background="@null"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Welcome!"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="40sp"
        android:textStyle="bold|italic"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button"
        android:layout_width="165dp"
        android:layout_height="65dp"
        android:layout_marginTop="236dp"
        android:background="@drawable/button_setup"
        android:text="Sign in"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button2"
        android:layout_width="165dp"
        android:layout_height="65dp"
        android:layout_marginTop="320dp"
        android:background="@drawable/button_setup"
        android:text="Sign Up"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/news"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:background="@drawable/round_button"
        android:padding="15dp"
        android:layout_centerHorizontal="true"
        android:scaleType="fitCenter"
        android:layout_marginLeft="38dp"
        android:layout_marginTop="489dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/icon1"/>

    <TextView
        android:id="@+id/editTextTextPersonName5"
        android:layout_width="33dp"
        android:layout_height="17dp"
        android:layout_marginLeft="64dp"
        android:layout_marginTop="570dp"
        android:background="@null"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="News"
        android:textColor="#000000"
        android:textSize="11sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="TouchTargetSizeCheck" />

    <ImageButton
        android:id="@+id/partners"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:background="@drawable/round_button2"
        android:padding="15dp"
        android:layout_centerHorizontal="true"
        android:scaleType="fitCenter"
        android:layout_marginTop="444dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/icon2"/>

    <TextView
        android:id="@+id/editTextTextPersonName6"
        android:layout_width="64dp"
        android:layout_height="21dp"
        android:layout_marginTop="528dp"
        android:background="@null"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Our partners"
        android:textColor="#000000"
        android:textSize="11sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="TouchTargetSizeCheck" />

    <ImageButton
        android:id="@+id/about"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:background="@drawable/round_button"
        android:padding="15dp"
        android:layout_centerHorizontal="true"
        android:scaleType="fitCenter"
        android:layout_marginRight="38dp"
        android:layout_marginTop="489dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/icon3"/>

    <TextView
        android:id="@+id/editTextTextPersonName7"
        android:layout_width="44dp"
        android:layout_height="19dp"
        android:layout_marginTop="570dp"
        android:layout_marginRight="56dp"
        android:background="@null"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="About us"
        android:textColor="#000000"
        android:textSize="11sp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/editTextTextPersonName4"
        android:layout_width="74dp"
        android:layout_height="20dp"
        android:layout_marginLeft="120dp"
        android:layout_marginTop="623dp"
        android:background="@null"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="New User?"
        android:textColor="#AAAAAA"
        android:textSize="14sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="TouchTargetSizeCheck" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button6"
        android:layout_width="99dp"
        android:layout_height="18dp"
        android:layout_marginLeft="195dp"
        android:layout_marginTop="623dp"
        android:background="@null"
        android:gravity="left"
        android:text="Sign Up now!"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/editTextTextPersonName3"
        android:layout_width="412dp"
        android:layout_height="132dp"
        android:background="#C8C8C8"
        android:ems="10"
        android:gravity="top"
        android:paddingLeft="10dp"
        android:paddingTop="10dp"
        android:text="Tip of the day:\nWhen your muscles hurt, make sure you give them enough time to recover. Taking a rest for a day or two won't make your results vanish!"
        android:textColor="#000000"
        android:textSize="14sp"
        android:textStyle="italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>